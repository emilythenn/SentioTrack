class PhysicalWidget extends StatefulWidget {
  const PhysicalWidget({super.key});

  static String routeName = 'physical';
  static String routePath = '/physical';

  @override
  State<PhysicalWidget> createState() => _PhysicalWidgetState();
}

class _PhysicalWidgetState extends State<PhysicalWidget> {
  late PhysicalModel _model;

  final scaffoldKey = GlobalKey<ScaffoldState>();

  @override
  void initState() {
    super.initState();
    _model = createModel(context, () => PhysicalModel());

    // On page load action.
    SchedulerBinding.instance.addPostFrameCallback((_) async {
      _model.apiResult5t5 = await GetSensorDataCall.call();

      if ((_model.apiResult5t5?.succeeded ?? true)) {
        _model.stepscount = getJsonField(
          (_model.apiResult5t5?.jsonBody ?? ''),
          r'''$.steps''',
        );
        safeSetState(() {});
      }
    });
  }

  @override
  void dispose() {
    _model.dispose();

    super.dispose();
  }

